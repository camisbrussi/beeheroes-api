{
  "openapi": "3.0.0",
  "info": {
    "title": "BeeHeroes Documentation",
    "description": "This is a Beeheroes system api",
    "version": "1.0.0",
    "contact": {
      "email": "camila.sbrussi@universo.univates.br"
    }
  },
  "paths": {
    "/entitytypes": {
      "post": {
        "tags": ["Entity Type"],
        "summary": "Create a Entity Type",
        "description": "Create a New Entity Type",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string"
                  },
                  "description": {
                    "type": "string"
                  }
                },
                "example": {
                  "name": "entity type name sample",
                  "description": "entity type description sample"
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Created"
          },
          "500": {
            "description": "Entity Type already exists!"
          }
        }
      }
    },
    "/usertypes": {
      "post": {
        "tags": ["User Type"],
        "summary": "Create a User Type",
        "description": "Create a New User Type",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string"
                  },
                  "description": {
                    "type": "string"
                  }
                },
                "example": {
                  "name": "user type name sample",
                  "description": "user type description sample"
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Created"
          },
          "500": {
            "description": "User Type already exists!"
          }
        }
      }
    },
    "/users": {
      "post": {
        "tags": ["User"],
        "summary": "Create a User",
        "description": "Create a New User",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string"
                  },
                  "email": {
                    "type": "string"
                  },
                  "password": {
                    "type": "string"
                  },
                  "user_type_id": {
                    "type": "string"
                  }
                },
                "example": {
                  "name": "user type name sample",
                  "email": "beeheroes@example.com",
                  "password": "123456",
                  "user_type_id": "ba863673-b4e0-42d2-9fea-41d98e9d541a"
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Created"
          },
          "500": {
            "description": "User already exists!"
          }
        }
      }
    }
  }
}